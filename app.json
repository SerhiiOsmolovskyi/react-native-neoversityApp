{
  "expo": {
    "name": "NeoversityApp",
    "slug": "NeoversityApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png ",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "plugins": [
        [
          "expo-camera",
          {
            "cameraPermission": "Allow $(NeoversityApp) to access your camera",
            "microphonePermission": "Allow $(NeoversityApp) to access your microphone",
            "recordAudioAndroid": true
          }
        ]
      ]
    },
    "assetBundlePatterns": [
      "assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.osmol.NeoversityApp"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "6790c6f5-4dc6-44ab-9eb0-fd004d3b8488"
      }
    }
  }
}
